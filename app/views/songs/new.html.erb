<h3> "Create a Song" </h3>
<%= form_for(@song) do |f|%>
    <%= f.label 'Title:'%><br>
    <%= f.text_field :title %><br>
    <%= f.label 'Year:'%><br>
    <%= f.text_field :release_year%><br>
    <%= f.label 'Artist:'%><br>
    <%= f.text_field :artist_name %><br>
    <%= f.label 'Genre:'%><br>
    <%= f.text_field :genre%><br>
    <%= f.submit %><br>
<% end %>